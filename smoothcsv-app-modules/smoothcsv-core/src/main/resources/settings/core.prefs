# General
language=<NULL>

# Appearance
statusbarVisible=true
toolbarVisible=false
valuepanelVisible=true
defaultFontSize=13
statusBarFontSize=12
gridFontSize=13
valuePanelFontSize=13
cellPaddingTop=1
cellPaddingBottom=1

# File
alertOnOpeningHugeFile=true
alertThreshold=50
howToDetectProperties=auto

# Editor
defaultRowSize=5
defaultColumnSize=5
