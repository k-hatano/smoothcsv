rowSizeToScanWhenAutoFitting=1000
limitWidthWhenAutoFitting=true
maxColumnWidthPerWindowWhenAutoFitting=70

#Copy & Paste
quoteRuleForCopying=QUOTES_IF_NECESSARY
pasteRepeatedly=true
